{
  "name": "electronnationalizedbuild",
  "version": "1.0.0",
  "author": "nandazzia <597974450@qq.com>",
  "description": "4.2.11_230530_stable",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "executableName":"ElectronNationalizedBuild",
  "homepage": "./",
  "scripts": {
    "dev": "cross-env TERGET_ENV=development node .electron-vue/dev-runner.js",
    "build": "cross-env BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --win  --x64 --config electron-builder-kylin.json",
    "build:kylinarm64": "cross-env BUILD_TARGET=clean node .electron-vue/build.js  && electron-packager ./   --platform=linux --arch=arm64 --out build/ ",
    "build:debarm64": "electron-installer-debian --src build/ElectronNationalizedBuild-linux-arm64 --dest ./deb/ --arch arm64 --config electron-packager-arm64.json",
    "build:kylinx64": "cross-env sudo BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --linux --config electron-builder-kylin.json",
    "build:uosx64": "cross-env sudo BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --linux --config uos-builder.json ",
    "build:win32": "cross-env BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --win  --ia32",
    "build:win64": "cross-env BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --win  --x64 --config electron-builder-kylin.json",
    "build:mac": "cross-env BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --mac",
    "build:dir": "cross-env BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=onlyClean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "pack:resources": "node .electron-vue/hot-updater.js",
    "update:serve": "node server/index.js",
    "dep:upgrade": "yarn upgrade-interactive --latest",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "echarts": "^5.3.3",
    "electron-reloader": "^1.2.3",
    "electron-updater": "^5.0.1",
